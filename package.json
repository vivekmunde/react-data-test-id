{
  "name": "react-data-test-id",
  "version": "0.1.0",
  "description": "DataTestId attribute setter for React applications",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "rimraf coverage && vitest run --coverage --config vitest-config.ts",
    "test:coverage": "rimraf coverage && vitest run --coverage --config vitest-config.ts"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.1",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitest/coverage-v8": "^1.6.0",
    "jsdom": "^24.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0"
  }
}
